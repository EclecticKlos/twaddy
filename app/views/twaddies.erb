<% followees_tweets = [] %>
<% User.find(params[:session][:user_id]).followees.each {|followee| followee.tweets.each {|tweet| followees_tweets << tweet}} %>
<ul>
  <% followees_tweets.each do |tweet| %>
    <li><%= tweet %>/<li>
  <% end %>
</ul>






